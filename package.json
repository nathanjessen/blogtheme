{
  "name": "blogtheme",
  "version": "1.1.0",
  "description": "Clean minimalist theme for Jekyll using TailwindCSS.",
  "type": "module",
  "main": "gulpfile.js",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^3.0.3",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.6",
    "cssnano": "^6.0.2",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.4.0"
  },
  "scripts": {
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose up",
    "docker:dev:rebuild": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "build:production": "cross-env NODE_ENV=production docker-compose run --rm jekyll npm run build:docker",
    "build:dev": "cross-env NODE_ENV=development docker-compose run --rm jekyll npm run build:docker",
    "build:docker": "cross-env NODE_ENV=production gulp",
    "build": "npm run build:production",
    "dev": "npm run docker:dev",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanjessen/blogtheme.git"
  },
  "author": "Nathan Jessen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanjessen/blogtheme/issues"
  },
  "homepage": "https://github.com/nathanjessen/blogtheme#readme",
  "publishConfig": {
    "access": "public"
  }
}
